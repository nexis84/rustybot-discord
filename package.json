{
    "name": "rustybot-discord-bot",
    "version": "1.0.0",
    "description": "RustyBot - EVE Online Discord Market Bot with real-time market data, manufacturing costs, and LP analysis",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "node server.js",
        "dev": "node --watch server.js",
        "docker:build": "docker build -t rustybot-discord-bot .",
        "docker:run": "docker run --env-file .env -p 8080:8080 rustybot-discord-bot",
        "test": "echo \"Error: no test specified\" && exit 1",
        "health": "curl -f http://localhost:8080/health || exit 1"
    },
    "keywords": [
        "discord",
        "bot",
        "eve-online",
        "market",
        "rustybot",
        "gaming",
        "api",
        "real-time"
    ],
    "author": "",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nexis84/rustybot-discord-bot.git"
    },
    "bugs": {
        "url": "https://github.com/nexis84/rustybot-discord-bot/issues"
    },
    "homepage": "https://github.com/nexis84/rustybot-discord-bot#readme",
    "engines": {
        "node": ">=16.0.0"
    },
    "dependencies": {
        "axios": "^1.11.0",
        "bottleneck": "^2.19.5",
        "discord.js": "^14.22.1",
        "dotenv": "^17.2.2",
        "express": "^5.1.0",
        "web-streams-polyfill": "^4.2.0"
    }
}
